<h1>Edit Article</h1>
<form action="/articles/<%= @article.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="PATCH">
  Article Name: <input type="text" name="title" value="<%= @article.title %>"><br>
  Article Content: <input type="text" name="content" value="<%= @article.content %>"><br>
  <input type="submit" />
</form>
